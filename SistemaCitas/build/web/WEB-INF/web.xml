<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GuardarCitaServlet</servlet-name>
        <servlet-class>Servlets.GuardarCita</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>expedienteHabitante</servlet-name>
        <servlet-class>Servlets.expedienteHabitante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>expedienteIndividual</servlet-name>
        <servlet-class>Servlets.expedienteIndividual</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrarCita</servlet-name>
        <servlet-class>Servlets.registrarCita</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InicioSesion</servlet-name>
        <servlet-class>Servlets.InicioSesion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GuardarCitaServlet</servlet-name>
        <url-pattern>/GuardarCita</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>expedienteHabitante</servlet-name>
        <url-pattern>/expedienteHabitante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>expedienteIndividual</servlet-name>
        <url-pattern>/expedienteIndividual</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrarCita</servlet-name>
        <url-pattern>/registrarCita</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InicioSesion</servlet-name>
        <url-pattern>/InicioSesion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
